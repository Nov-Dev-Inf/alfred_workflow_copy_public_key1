				<string>#!/bin/bash

SSHKEY_FILENAME="{query}".pub
if [ ! -f ~/.ssh/$SSHKEY_FILENAME ]; then
	SSHKEY_FILENAME=id_rsa.pub
fi

cat ~/.ssh/$SSHKEY_FILENAME | pbcopy

echo "copied $SSHKEY_FILENAME"</string>
